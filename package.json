  "devDependencies": {
    "electron": "^29.1.0",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "node-tesseract-ocr": "^2.2.1",
    "speaker": "^0.5.5",
    "tesseract.js": "^6.0.1"
  }
}
