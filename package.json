{
  "name": "real-screen-reader",
  "version": "1.0.0",
  "description": "A screen reader application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron .",
    "build": "electron-builder build",
    "package": "electron-builder --dir"
